pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                echo 'Building the project'
                echo 'Finished building the project'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing the project'
                echo 'Finished testing the project'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying the project'
                echo 'Finished deploying the project'
            }
        }
    }
}